	        <result type="json">
		        <param name="noCache">true</param>
		        <param name="root">jsonString</param>
	        </result>	        
		</action>
		<action name="getHorarioByPrestador" class="prestadores.action.json" method="writeJSONHorario">
	        <result type="json">
		        <param name="noCache">true</param>
		        <param name="root">jsonString</param>
	        </result>	        
		</action>
		
	</package>
     
</struts>